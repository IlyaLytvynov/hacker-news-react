    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "graphql": "^14.0.2",
    "historyjs": "^1.8.0-b2",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mobx": "^5.1.2",
    "mobx-react": "^5.2.8",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
